test "@test6.AutoGenerated::to_json works as expected" {
  let got = ag.to_json().stringify(escape_slash=false)
  assert_eq!(got, want_ag)
}

test "@json.parse(@test6.AutoGenerated) works as expected" {
  let jv = @json.parse!(want_ag)
  let got : @test6.AutoGenerated = @json.from_json!(jv)
  assert_eq!(got, ag)
}

let ag : @test6.AutoGenerated = {
  people: [
    { name: "Frank" },
    { name: "Dennis" },
    { name: "Dee" },
    { name: "Charley" },
    { name: "Mac" },
  ],
}

let want_ag =
  #|{"people":[{"name":"Frank"},{"name":"Dennis"},{"name":"Dee"},{"name":"Charley"},{"name":"Mac"}]}
